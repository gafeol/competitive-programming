struct arv{
    ll d;
    int lzl;
    ll dl;
} tree[MAXN*4];


void build(int idx, int i, int j){
    tree[idx].mn = 0;
    tree[idx].lzl = -1;
    tree[idx].dl = 0;
    if(i == j)
        return ;
    int m = (i + j)/2;
    build(idx*2,i,m);
    build(idx*2+1,m+1, j);
}

void unlaze(int idx, int i, int j){
